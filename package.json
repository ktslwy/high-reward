    "moment": "^2.10.6"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "chai": "^2.2.0",
    "eslint": "^1.5.0",
    "mocha": "^2.3.3",
    "spritesheet-js": "^1.1.4",
    "text-table": "0.2.0",
    "wzrd": "^1.3.1"
